  "scripts": {
    "compile": "TS_NODE_IGNORE='' hardhat compile",
    "test": "yarn compile && TS_NODE_TRANSPILE_ONLY=1 TS_NODE_IGNORE='' mocha --extension ts --require hardhat/register --require @balancer-labs/v2-common/setupTests --recursive",
    "postinstall": "yarn symlink-artifacts",
    "symlink-artifacts": "ln -srfn artifacts/@balancer-labs/v2-pool-weighted node_modules/@balancer-labs/v2-pool-weighted/artifacts && ln -srfn artifacts/@balancer-labs/v2-pool-utils node_modules/@balancer-labs/v2-pool-utils/artifacts && ln -srfn artifacts/@balancer-labs/v2-standalone-utils node_modules/@balancer-labs/v2-standalone-utils/artifacts && ln -srfn artifacts/@balancer-labs/v2-vault node_modules/@balancer-labs/v2-vault/artifacts",
    "typechain": "npx typechain --target ethers-v5 'build/{contracts,interfaces}/*.json' --outDir typechain",
    "prettier": "prettier --write 'contracts/**/*.sol'",
    "lint": "prettier --list-different 'contracts/**/*.sol' && black --check tests"
  }
